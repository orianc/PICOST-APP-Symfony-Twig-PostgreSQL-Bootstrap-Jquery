<form method="post" action="{{ path('photo_delete', {'id': photo.id}) }}" onsubmit="return confirm('Are you sure you want to delete this pic ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete') }}">
	<button class="btn btn-danger">
		<i class="fas fa-trash-alt"></i>
	</button>
</form>
